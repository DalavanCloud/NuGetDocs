<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@Page.Title</title>
    <link href="~/content/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="~/css/simple-sidebar.css" rel="stylesheet" type="text/css" />
    <link href="~/css/nuget.docs.css?2015-02-16" rel="stylesheet" type="text/css" />
    <link href="~/css/nanoscroller.css" rel="stylesheet" type="text/css" />
    <link href="~/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/css/toc.css" rel="stylesheet">
    <link href="~/css/highlight/solarized-dark.css" rel="stylesheet">
</head>
<body>
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand navmods" href="http://nuget.org" onclick="trackOutboundLink('http://nuget.org'); return false;"><span><img class="brandimage" src="~/images/nugetBrand.png" /></span>nuget</a>
            </div>
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li><a href="https://www.nuget.org/packages" onclick="trackOutboundLink('https://www.nuget.org/packages'); return false;">Packages</a></li>
                    <li><a href="https://docs.microsoft.com/nuget/">Documentation</a></li>
                    <li class="visible-lg visible-xs"><a href="https://dist.nuget.org/index.html" onclick="trackOutboundLink('https://dist.nuget.org/index.html'); return false;">Downloads</a></li>
                    <li class="visible-lg visible-xs"><a href="https://docs.microsoft.com/nuget/release-notes/index">Release Notes</a></li>
                    <li class="visible-lg visible-xs"><a href="http://blog.nuget.org/" onclick="trackOutboundLink('http://blog.nuget.org/'); return false;">Blog</a></li>
                    <li class="visible-lg visible-xs"><a href="https://www.nuget.org/policies/contact" onclick="trackOutboundLink('https://www.nuget.org/policies/contact'); return false;">Contact Us</a></li>
                </ul>
                <div class="search">
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <form class="navbar-form" role="search" action="~/search">
                                <div class="input-group">
                                    <input type="text" class="form-control input-lg hidden-xs" placeholder="search docs" autocomplete="off" name="q" id="srch-term" value="@PageData["query"]" method="get" novalidate="novalidate">
                                    <div class="input-group-btn">
                                        <button class="btn btn-default searchButton hidden-xs" type="submit"><i class="glyphicon glyphicon-search"></i></button>
                                        <a href="#menu-toggle" class="btn btn-default toggleTocButton visible-xs" id="menu-toggle">Toggle TOC</a>
                                    </div>
                                </div>
                            </form>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    <div id="wrapper">
        <div id="sidebar-wrapper" style="background-color:transparent">
            <div id="about" class="nano">
                <div class="nano-content">
                    <div id="listContainer" class="listDataSet">
                        <ul id="expList">
                            @TopicHelper.RenderDocumentationIndex()
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div id="page-content-wrapper">
            @RenderBody()
        </div>
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/modernizr/modernizr-2.7.2.js"></script>
    <script src="~/js/nuget.docs.js"></script>
    <script src="~/Scripts/nanoscoller.min.js"></script>
    <script src="~/scripts/highlight.pack.js"></script>
    <script src="~/Scripts/clipboard.min.js"></script>
    <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
        
          ga('create', 'UA-30016961-4', 'auto');
          ga('send', 'pageview');

        var trackOutboundLink = function(url) {
           ga('send', 'event', 'outbound', 'click', url, {
             'transport': 'beacon',
             'hitCallback': function(){document.location = url;}
           });
        }
    </script>
</body>
</html>
